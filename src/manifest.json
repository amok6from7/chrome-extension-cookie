{
  "name": "cookie-extension",
  "description": "chrome extension",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/cookie.png",
    "128": "icons/cookie.png"
  },
  "browser_action": {
    "default_title": "cookie-extension",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "cookies",
    "<all_urls>"
  ]
}
